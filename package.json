{
  "name": "<name>",
  "version": "1.0.0",
  "description": "<description>",
  "main": "src/index.ts",
  "scripts": {
    "pre-commit": "lint-staged && tsc --noEmit",
    "pre-push": "npm test",
    "prepare": "npm run husky:init",
    "husky:init": "husky install && husky set .husky/pre-commit \"npm run pre-commit\" && husky set .husky/pre-push \"npm run pre-push\"",
    "build": "tsc --project tsconfig.production.json",
    "dev": "cross-env NODE_ENV=development node dist/index.js",
    "dev:once": "npm run build && npm run dev",
    "dev:watch": "nodemon",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts config/*.json --fix --cache",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "<repository-url>"
  },
  "author": "<author>",
  "license": "ISC",
  "dependencies": {
    "@types/config": "3.3.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.3",
    "@types/winston": "^2.4.4",
    "config": "^3.3.9",
    "typescript": "^5.0.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0"
  }
}
